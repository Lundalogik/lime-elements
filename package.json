{
  "name": "lime-elements",
  "version": "15.0.3",
  "description": "Lime Elements",
  "author": "Lime Technologies",
  "license": "UNLICENSED",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "postinstall.js"
  ],
  "scripts": {
    "postinstall": "node postinstall.js",
    "build": "cross-env-shell NODE_ENV=prod SASS_PATH=node_modules \"stencil build --config stencil.config.dist.ts\"",
    "cm": "git-cz",
    "dev": "cross-env-shell SASS_PATH=node_modules \"stencil build --dev --docs\"",
    "dev:watch": "cross-env-shell SASS_PATH=node_modules \"stencil build --dev --watch --docs\"",
    "dev:es5": "cross-env-shell SASS_PATH=node_modules \"stencil build --dev --docs --es5\"",
    "dev:es5:watch": "cross-env-shell SASS_PATH=node_modules \"stencil build --dev --watch --docs --es5\"",
    "docz:build": "docz build && cross-env-shell NODE_ENV=prod SASS_PATH=node_modules \"stencil build --docs --config stencil.config.docs.ts\"",
    "docz:dev": "docz dev",
    "docz:serve": "serve .docz/dist --single",
    "docz:publish": "node publish-docs.js",
    "lint": "npm run lint:src && npm run lint:specs && npm run lint:examples && npm run lint:config",
    "lint:fix": "npm run lint:src:fix && npm run lint:specs:fix && npm run lint:examples:fix && npm run lint:config:fix",
    "lint:config": "npx tslint --project . -t stylish -c tslint.config.js '*.{ts,tsx}' && npx eslint -c eslint.js '*.js' && npx prettier -l '{!(package*).json,.prettierrc}'",
    "lint:config:fix": "npx tslint -t stylish --fix -c tslint.config.js '*.{ts,tsx}' && npx eslint --fix -c eslint.js '*.js' && npx prettier --write '{!(package*).json,.prettierrc}'",
    "lint:examples": "npx tslint --project . -t stylish -c tslint.examples.js 'src/examples/**/*.{ts,tsx}'",
    "lint:examples:fix": "npx tslint --project . -t stylish --fix -c tslint.examples.js 'src/examples/**/*.{ts,tsx}'",
    "lint:specs": "npx tslint --project . -t stylish -c tslint.specs.js 'src/**/*.{spec,e2e}.{ts,tsx}'",
    "lint:specs:fix": "npx tslint --project . -t stylish --fix -c tslint.specs.js 'src/**/*.{spec,e2e}.{ts,tsx}'",
    "lint:src": "npx tslint --project . -t stylish -c tslint.js 'src/**/*.{ts,tsx}'",
    "lint:src:fix": "npx tslint --project . -t stylish --fix -c tslint.js 'src/**/*.{ts,tsx}'",
    "release": "npm run build && semantic-release",
    "start": "concurrently \"npm run dev:watch\" \"docz dev\"",
    "start:es5": "npm run dev:es5 && concurrently \"npm run dev:es5:watch\" \"docz dev\"",
    "test": "cross-env-shell SASS_PATH=node_modules \"stencil test --spec --e2e\"",
    "test:e2e": "cross-env-shell SASS_PATH=node_modules \"stencil test --e2e\"",
    "test:e2e:watch": "cross-env-shell SASS_PATH=node_modules \"stencil test --e2e --watch\"",
    "test:specs": "stencil test --spec",
    "test:specs:watch": "stencil test --spec --watch"
  },
  "dependencies": {
    "awesome-debounce-promise": "^1.0.0",
    "flatpickr": "^4.5.2",
    "jsx-dom": "^6.2.1",
    "lime-material-components-web": "0.43.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@stencil/core": "^0.17.0",
    "@stencil/sass": "^0.1.1",
    "@types/jest": "^23.3.13",
    "@types/puppeteer": "1.11.2",
    "commitizen": "^3.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.12.9",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "lodash-es": "^4.17.11",
    "ncp": "^2.0.0",
    "pixelmatch": "4.0.2",
    "prettier": "^1.16.1",
    "prismjs": "^1.15.0",
    "puppeteer": "1.11.0",
    "replace-in-file": "^3.4.3",
    "semantic-release": "^15.13.3",
    "serve": "^10.1.1",
    "shelljs": "^0.8.3",
    "showdown": "^1.9.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tslint-sonarts": "^1.9.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npm run docz:publish -- --v=${nextRelease.version} --dryRun=${options.dryRun}"
        }
      ]
    ],
    "npmPublish": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lundalogik/lime-elements.git"
  },
  "publishConfig": {
    "registry": "http://npm.lundalogik.com:4873/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
